    "automated",
    "tasks",
    "processor"
  ],
  "dependencies": {
    "graphql-request": "^1.8.2",
    "js-yaml": "^3.13.0",
    "lodash": "^4.17.11",
    "memorystream": "^0.3.1",
    "promise-streams": "^2.1.1",
    "query-string": "^6.4.2",
    "readdirp": "^2.2.1",
    "through2": "^3.0.1",
    "typescript": "^3.4.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "@types/should": "^13.0.0",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "should": "^13.2.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "tsctests": "tsc test/*.ts --module commonjs -target ES6",
    "pretest": "npm run tsctests && npm run lint",
