{
  "name": "@restorecommerce/gql-bot",
  "description": "GraphQL Client automated tasks processor",
  "main": "lib/index",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "http://github.com/restorecommerce/gql-bot.git"
  },
  "license": "MIT",
  "keywords": [
    "graphQL",
    "client",
    "automated",
    "tasks",
    "processor"
  ],
  "dependencies": {
    "graphql-request": "^1.8.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.14",
    "memorystream": "^0.3.1",
    "promise-streams": "^2.1.1",
    "query-string": "^6.8.1",
    "readdirp": "^3.1.1",
    "through2": "^3.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.5",
    "@types/should": "^13.0.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "tsctests": "tsc test/*.ts --module commonjs -target ES6",
    "pretest": "npm run tsctests && npm run lint",
    "test": "nyc mocha",
    "lint": "tslint './src/**/*.ts'",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc",
    "postinstall": "tsc -d",
    "mocha": "mocha --timeout 5000 ./test/*.js --trace-warnings",
    "mocha-debug": "mocha -R spec test/*.js --full-trace --inspect-brk",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
